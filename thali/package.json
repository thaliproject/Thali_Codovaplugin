    "bluebird": "3.4.6",
    "body-parser": "1.13.3",
    "express": "4.13.3",
    "forever-agent": "https://github.com/thaliproject/forever-agent.git#f1078b5095dfe00ad0d80779eba75e563b54608b",
    "ip": "1.0.1",
    "javascript-state-machine": "2.3.5",
    "lie": "3.1.0",
    "long": "3.0.3",
    "multiplex": "6.7.0",
