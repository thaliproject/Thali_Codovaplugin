    "uuid": "2.0.2",
    "winston": "2.2.0",
    "js-extend": "1.0.1"
  },
  "devDependencies": {
    "jsdoc": "3.3.3",
    "eslint": "3.11.1"
  },
  "thaliInstall": {
    "thali": {
      "projectName": "thaliproject",
      "depotName": "Thali_CordovaPlugin",
      "branchName": "iOS"
    },
    "androidConfig": {
      "minSdkVersion": "21",
      "buildToolsVersion": "25.0.0",
      "compileSdkVersion": "android-23"
    },
    "btconnectorlib2": "0.3.7-alpha",
    "jxcore-cordova": "0.1.8",
